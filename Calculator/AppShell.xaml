<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Calculator.AppShell"
       xmlns:local="clr-namespace:Calculator">
    <FlyoutItem Title="Calculator" >
        <ShellContent Title="Calculator"
                      ContentTemplate="{DataTemplate local:MainPage}"
                      Route="mainpage"/>
    </FlyoutItem>
    <FlyoutItem Title="Previous Calculations">
        <ShellContent Title="Previous Calculations"
            ContentTemplate="{DataTemplate local:CalcHistoryPage}"
                      Route="calchistorypage"/>
    </FlyoutItem>
</Shell>
